#!/bin/bash

/usr/bin/rm -f /usr/share/fbpanel/images/logo.png.backup
/usr/bin/mv -f /usr/share/fbpanel/images/logo.png /usr/share/fbpanel/images/logo.png.backup
/usr/bin/cp -f /usr/share/fbpanel/images/archlinux-logo.png /usr/share/fbpanel/images/logo.png
/usr/bin/chmod 644 /usr/share/fbpanel/images/logo.png
